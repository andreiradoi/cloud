{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "d3": "4.13.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.1",
    "mapbox-gl": "0.44.1",
    "moment": "^2.19.2",
    "pondjs": "^0.8.8",
    "prop-types": "^15.6.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^15.5.4",
    "react-mapbox-gl": "^3.8.0",
    "react-radial": "^1.0.8",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.1",
    "react-timeseries-charts": "^0.12.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-preset-flow": "^6.23.0",
    "chokidar-cli": "^1.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.46.0",
    "node-sass": "^4.5.2",
    "prettier": "^1.3.1",
    "react-scripts": "^1.1.1"
  },
  "scripts": {
    "start": "yarn sass-build && react-scripts start",
    "build": "yarn sass-build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass-watch": "chokidar 'src/sass/**/*.scss' --initial -c 'yarn run sass-build'",
    "sass-build": "node-sass --recursive src/sass --output src/css --source-map true",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "pretty": "prettier --single-quote --trailing-comma es5 --jsx-bracket-same-line=true --parser flow --write \"src/**/*.js\"",
    "fix": "yarn pretty && yarn lint -- --fix"
  }
}
