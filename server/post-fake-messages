#!/bin/bash

function rock_block() {
    DATA=$1
    MESSAGE="device_type=ROCKBLOCK&serial=11380&momsn=1086&transmit_time=17-08-06+12%3A01%3A14&imei=300234065114240&iridium_latitude=-12.7071&iridium_longitude=15.5300&iridium_cep=302&data=$DATA"
    curl -H "Content-Type: application/x-www-form-urlencoded; charset=UTF-8" -X POST -d $MESSAGE http://127.0.0.1:8080/messages/ingestion
}

